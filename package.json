{
  "name": "slidev-theme-ebpf-vienna",
  "version": "0.0.1",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "ebpf",
    "vienna"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.51.0-beta.4",
    "slidev-addon-qrcode": "^1.0.2",
    "slidev-component-spotlight": "^1.1.0"
  },
  "devDependencies": {
    "@slidev/cli": "^0.51.0-beta.4",
    "playwright-chromium": "^1.50.1"
  },
  "slidev": {
    "colorSchema": "both",
    "addons": [
      "slidev-component-spotlight",
      "slidev-addon-qrcode"
    ],
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}
