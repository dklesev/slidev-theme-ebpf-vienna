{
  "name": "@dklesev/slidev-theme-ebpf-vienna",
  "version": "0.0.11",
  "description": "A Slidev theme for eBPF Vienna meetup presentations",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "ebpf",
    "vienna",
    "presentation",
    "slides"
  ],
  "license": "MIT",
  "author": {
    "name": "Dimitrij Klesev",
    "url": "https://github.com/dklesev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dklesev/slidev-theme-ebpf-vienna.git"
  },
  "bugs": {
    "url": "https://github.com/dklesev/slidev-theme-ebpf-vienna/issues"
  },
  "homepage": "https://github.com/dklesev/slidev-theme-ebpf-vienna#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=24.4.0"
  },
  "files": [
    "layouts",
    "components",
    "styles",
    "setup",
    "public",
    "global-top.vue",
    "global-bottom.vue",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.11.3",
    "slidev-addon-qrcode": "^1.0.2",
    "slidev-component-spotlight": "^1.1.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.11.3",
    "playwright-chromium": "^1.57.0"
  },
  "slidev": {
    "colorSchema": "both",
    "addons": [
      "slidev-component-spotlight",
      "slidev-addon-qrcode"
    ],
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}
